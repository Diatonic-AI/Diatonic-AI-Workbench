# Development Environment Configuration
VITE_NODE_ENV=development

# AWS Configuration
VITE_AWS_REGION=us-east-2

# AWS Cognito Configuration (will be populated by Terraform)
VITE_AWS_COGNITO_USER_POOL_ID=
VITE_AWS_COGNITO_USER_POOL_CLIENT_ID=
VITE_AWS_COGNITO_IDENTITY_POOL_ID=
VITE_AWS_COGNITO_USER_POOL_DOMAIN=

# API Gateway Configuration
VITE_API_GATEWAY_URL=
VITE_API_GATEWAY_STAGE=dev

# S3 Configuration
VITE_S3_BUCKET_NAME=
VITE_S3_REGION=us-east-2
VITE_CLOUDFRONT_DOMAIN=

# DynamoDB Configuration
VITE_DYNAMODB_USER_DATA_TABLE=
VITE_DYNAMODB_FILES_TABLE=
VITE_DYNAMODB_SESSIONS_TABLE=

# Application Configuration
VITE_APP_NAME="AI Nexus Workbench"
VITE_APP_VERSION=1.0.0
VITE_APP_DOMAIN=localhost:8080
VITE_APP_URL=http://localhost:8080

# Feature Flags
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_LOGGING=true
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Security Configuration
VITE_ENABLE_CSP=false
VITE_ENABLE_HSTS=false
VITE_ENABLE_SECURE_HEADERS=false
