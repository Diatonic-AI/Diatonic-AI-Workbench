{
  "name": "ai-nexus-workbench-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for AI Nexus Workbench - AI Lab & Toolset Backend",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "deploy:dev": "cdk deploy --context environment=dev --context account=863584456189 --context region=us-east-2",
    "deploy:staging": "cdk deploy --context environment=staging --context account=863584456189 --context region=us-east-2",
    "deploy:prod": "cdk deploy --context environment=prod --context account=863584456189 --context region=us-east-2",
    "synth:dev": "cdk synth --context environment=dev --context account=863584456189 --context region=us-east-2",
    "synth:staging": "cdk synth --context environment=staging --context account=863584456189 --context region=us-east-2",
    "synth:prod": "cdk synth --context environment=prod --context account=863584456189 --context region=us-east-2",
    "lint": "eslint lib/**/*.ts",
    "format": "prettier --write lib/**/*.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-nexus-workbench.git"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "ai",
    "nexus",
    "workbench",
    "serverless",
    "lambda",
    "dynamodb",
    "cognito"
  ],
  "author": "AI Nexus Team",
  "license": "MIT"
}
