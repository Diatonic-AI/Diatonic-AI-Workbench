{
  "name": "diatonic-ai-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "./scripts/dev-prep.sh && vite",
    "dev:direct": "vite",
    "dev:background": "./scripts/launch-dev.sh --background",
    "dev:verify": "./scripts/launch-dev.sh --verify",
    "build": "npm run update-browserslist && vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "update-browserslist": "npm update caniuse-lite",
    "clean": "rm -rf dist node_modules/.vite",
    "prebuild": "npm run clean",
    "dynamodb:start": "./scripts/dynamodb-local.sh start",
    "dynamodb:stop": "./scripts/dynamodb-local.sh stop",
    "dynamodb:status": "./scripts/dynamodb-local.sh status",
    "dynamodb:reset": "./scripts/dynamodb-local.sh reset",
    "dynamodb:setup": "./scripts/dynamodb-local.sh setup",
    "dynamodb:admin": "./scripts/dynamodb-local.sh admin",
    "dynamodb:logs": "./scripts/dynamodb-local.sh logs",
    "dynamodb:sync-from-cloud": "./scripts/dynamodb-schema-sync.sh sync-from-cloud",
    "dynamodb:deploy-to-cloud": "./scripts/dynamodb-schema-sync.sh deploy-to-cloud",
    "dynamodb:validate": "./scripts/dynamodb-schema-sync.sh validate",
    "dynamodb:fix-config": "./scripts/dynamodb-schema-sync.sh fix-config",
    "dynamodb:schema-status": "./scripts/dynamodb-schema-sync.sh status",
    "db:migrate": "node scripts/setup-comprehensive-content.cjs",
    "setup-test-users": "npx tsx scripts/setup-test-users.ts",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "db:setup": "node scripts/setup-production-dynamodb.js",
    "db:setup:basic": "node scripts/setup-dynamodb-local.js",
    "db:reset": "npm run docker:down && npm run docker:up && sleep 5 && npm run db:setup",
    "dev:full": "npm run docker:up && sleep 5 && npm run db:setup && npm run dev",
    "api:start": "node scripts/start-api-server.mjs",
    "api:test:integration": "node scripts/test-api-integration.js",
    "api:test:endpoints": "node scripts/test-api-endpoints.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:auth": "vitest auth-integration.test.ts",
    "test:security": "jest --config tests/jest.config.mjs",
    "test:security:watch": "jest --config tests/jest.config.mjs --watch",
    "test:security:coverage": "jest --config tests/jest.config.mjs --coverage",
    "test:e2e:security": "playwright test --config playwright.config.ts",
    "test:e2e:security:ui": "playwright test --config playwright.config.ts --ui",
    "test:all-security": "npm run test:security:coverage && npm run test:e2e:security",
    "dev:with-api": "concurrently \"npm run api:start\" \"npm run dev\" --names \"API,WEB\" --prefix-colors \"blue,green\"",
    "test:integration": "node scripts/test-frontend-integration.js"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.12.0",
    "@aws-lambda-powertools/logger": "^2.25.2",
    "@aws-lambda-powertools/metrics": "^2.25.2",
    "@aws-lambda-powertools/tracer": "^2.25.2",
    "@aws-sdk/client-cognito-identity-provider": "^3.883.0",
