  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "./scripts/launch-dev.sh",
    "dev:direct": "vite",
    "dev:background": "./scripts/launch-dev.sh --background",
    "dev:verify": "./scripts/launch-dev.sh --verify",
    "build": "npm run update-browserslist && vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "update-browserslist": "npx update-browserslist-db@latest || npm update caniuse-lite",
    "clean": "rm -rf dist node_modules/.vite",
    "prebuild": "npm run clean",
    "dynamodb:start": "./scripts/dynamodb-local.sh start",
    "dynamodb:stop": "./scripts/dynamodb-local.sh stop",
    "dynamodb:status": "./scripts/dynamodb-local.sh status",
    "dynamodb:reset": "./scripts/dynamodb-local.sh reset",
    "dynamodb:setup": "./scripts/dynamodb-local.sh setup",
    "dynamodb:admin": "./scripts/dynamodb-local.sh admin",
    "dynamodb:logs": "./scripts/dynamodb-local.sh logs",
    "dynamodb:sync-from-cloud": "./scripts/dynamodb-schema-sync.sh sync-from-cloud",
    "dynamodb:deploy-to-cloud": "./scripts/dynamodb-schema-sync.sh deploy-to-cloud",
    "dynamodb:validate": "./scripts/dynamodb-schema-sync.sh validate",
    "dynamodb:fix-config": "./scripts/dynamodb-schema-sync.sh fix-config",
    "dynamodb:schema-status": "./scripts/dynamodb-schema-sync.sh status",
    "db:migrate": "node scripts/setup-comprehensive-content.cjs"
  },
