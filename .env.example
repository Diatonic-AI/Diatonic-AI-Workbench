# Environment Configuration
VITE_NODE_ENV=development

# AWS Configuration
VITE_AWS_REGION=us-east-2

# AWS Cognito Configuration
VITE_AWS_COGNITO_USER_POOL_ID=your-user-pool-id
VITE_AWS_COGNITO_USER_POOL_CLIENT_ID=your-user-pool-client-id
VITE_AWS_COGNITO_IDENTITY_POOL_ID=your-identity-pool-id

# API Gateway Configuration
VITE_AWS_API_GATEWAY_ENDPOINT=https://dev-api.diatonic.ai

# S3 Configuration
VITE_AWS_S3_BUCKET=ai-nexus-workbench-dev

# Application Configuration
VITE_APP_NAME=AI Nexus Workbench
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_DEBUG_LOGS=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_OFFLINE_MODE=false

# Domain Configuration
VITE_APP_DOMAIN=localhost:8080
